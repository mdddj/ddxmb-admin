{
  "name": "dd_blog_admin",
  "version": "2.0.0",
  "private": true,
  "description": "典典的小卖部后台管理平台, 博客平台",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "cross-env UMI_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-descriptions": "^1.10.46",
    "@ant-design/pro-form": "^1.55.0",
    "@ant-design/pro-layout": "^6.33.0",
    "@ant-design/pro-list": "^1.21.31",
    "@ant-design/pro-table": "^2.64.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@uiw/react-markdown-editor": "^3.2.7",
    "@umijs/hooks": "^1.9.3",
    "@umijs/route-utils": "^2.0.5",
    "antd": "^4.18.8",
    "classnames": "^2.3.1",
    "dd_server_api_web": "^1.7.6",

    "github-markdown-css": "^5.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "omit.js": "^2.0.2",
    "particles-bg": "^2.5.5",
    "pivotal-ui": "^19.2.5",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dev-inspector": "^1.7.1",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "react-markdown": "^8.0.0",
    "react-syntax-highlighter": "^15.4.5",
    "remark-gfm": "^3.0.1",
    "umi": "^3.5.21",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@playwright/test": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.11",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@umijs/fabric": "^2.10.2",
    "@umijs/openapi": "^1.4.0",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/plugin-openapi": "^1.3.2",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-dumi": "^1.1.38",
    "@umijs/preset-react": "^2.1.1",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^8.10.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-react": "^7.29.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "express": "^4.17.3",
    "gh-pages": "^3.2.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.3.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.5.1",
    "puppeteer-core": "^13.4.0",
    "stylelint": "^14.5.3",
    "swagger-ui-react": "^4.5.2",
    "typescript": "^4.5.5",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "__npminstall_done": false
}
